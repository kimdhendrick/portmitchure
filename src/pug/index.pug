                    a.mx-2(href='https://www.instagram.com/abeatingchest/')
                        i.fab.fa-instagram
                    a.mx-2(href='https://www.facebook.com/mitchell.barnes.54')
                        i.fab.fa-facebook-f
                    a.mx-2(href='mailto:mitchellbarnesphoto@gmail.com)
